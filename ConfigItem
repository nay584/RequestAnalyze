Public hostname As String

'01
Public manual01 As Boolean
Public division As String               '内容(新規or削除)
Public companyName As String            '会社名
Public groupName1 As String             'ホストグループ名1
Public groupName2 As String             'ホストグループ名2
Public pingMonitor As Boolean           'ピング監視　True or False
Public portMonitor As Boolean           'ポート監視  True or False
Public ip As String                     'IP Address
Private ports() As String               '監視ポート一覧

