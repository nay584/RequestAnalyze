Private templatesDelete() As String     'テンプレート名削除
Private macroNameAdd() As String        'マクロ変数追加
Private macroValueAdd() As String       'マクロ値追加
Private macroNameUpdate() As String     'マクロ変数変更
Private macroValueUpdate() As String    'マクロ値変更

